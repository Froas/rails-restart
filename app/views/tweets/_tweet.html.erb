<div id="<%= dom_id tweet %>" class="card-body">
  <p class="card-title">
    <strong>Name:</strong>
    <%= tweet.name %>
  </p>

  <p class="card-title">
    <strong>Title:</strong>
    <%= tweet.title %>
  </p>

  <p class="card-text">
    <strong>Blog:</strong>
    <%= tweet.blog %>
  </p>

</div>